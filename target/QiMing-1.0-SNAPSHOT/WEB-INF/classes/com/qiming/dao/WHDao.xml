<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiming.dao.WHDao">
    <!--添加-->
    <insert id="addWhq" parameterType="com.qiming.domain.Whq">
        insert into t_whq(id,title,time,content)values(#{id},#{title},#{time},#{content})
    </insert>
    <!--获取总条数-->
    <select id="whqGetCount" resultType="int">
        select count(*) from t_whq
    </select>
    <!--分页展示-->
    <select id="whqGetPageData" resultType="com.qiming.domain.Whq">
        select *from t_whq limit #{index} , 5
    </select>
    <!--查找所有-->
    <select id="whqAll" resultType="com.qiming.domain.Whq">
        select *from t_whq
    </select>
    <!--根据id查询-->
    <select id="whqFindById" resultType="com.qiming.domain.Whq">
        select *from t_whq where id=#{id}
    </select>

